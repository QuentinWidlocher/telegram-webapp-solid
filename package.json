{
  "name": "telegram-webapp-solid",
  "version": "0.7.4",
  "description": "SolidJS components for Telegram WebApps",
  "repository": {
    "type": "git",
    "url": "https://github.com/QuentinWidlocher/telegram-webapp-solid.git"
  },
  "scripts": {
    "dev": "vite --mode demo",
    "build": "vite build && tsc --emitDeclarationOnly; mv dist/src dist/types && cp -r src/types dist/types && rm -r dist/demo && rm dist/vite.config.d.ts",
    "build:demo": "vite build --mode demo",
    "serve": "vite preview --mode demo",
    "prepublish": "pnpm build"
  },
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "autoprefixer": "^10.4.13",
    "daisyui": "^2.46.1",
    "hex-to-hsl": "^1.0.2",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-solid": "^2.5.0"
  },
  "dependencies": {
    "solid-js": "^1.6.6"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/telegram-webapp-solid.umd.js",
  "module": "./dist/telegram-webapp-solid.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/telegram-webapp-solid.mjs",
      "require": "./dist/telegram-webapp-solid.umd.js"
    }
  }
}